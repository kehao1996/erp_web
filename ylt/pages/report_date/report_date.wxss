page{
  background: #f6f7fa;
}
.product{
  display: flex;
  justify-content: space-between;
  background: #fff;
  margin: 30rpx 20rpx 0 20rpx;
  border-top-right-radius: 30rpx;
  border-top-left-radius: 30rpx;
  padding: 0 20rpx;
  padding-bottom: 40rpx;
}
.product .product_ta{
  display: block;
  width: 146rpx;
  height: 62rpx;
  margin-top: 25rpx;
}
.product .product_tb{
  display: block;
  width: 445rpx;
  height: 445rpx;
  margin-top: 30rpx;
  border-radius: 25rpx;
  border: 1rpx solid #dce2ff;
}
.implement{
  margin: 0 20rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  padding-bottom: 20rpx;
}
.implement_a{
  display: flex;
  justify-content: space-between;  
}
.implement_a image{
  display: block;
  width: 146rpx;
  height: 62rpx;
}
.implement_a input{
  display: block;
  width: 212rpx;
  height: 62rpx;
  border: 1rpx solid #d8dfff;
  border-radius: 30rpx;
  background-color: #fff;
  font-size: 28rpx;
  text-align: center;
  margin-left: 20rpx;
}
.implement_a:last-child input{
  width: 100rpx;
}
/* 总计 */
.total{
  background-color: #fff;
  margin: 0 20rpx;
  padding: 0 20rpx;
  border-bottom-right-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
  padding-bottom: 30rpx;
}
.total_a{
  padding-bottom: 20rpx; 
  display: flex;
  justify-content: space-between;
}
.total .total_a image{
  width: 181rpx;
  height: 62rpx;
}
.total .total_a:nth-child(1) image{
  width: 216rpx;
  height: 62rpx;
}
.total .total_a:nth-child(2) image{
  width: 287rpx;
  height: 62rpx;
}
.total .total_a input{
  width: 340rpx;
  height: 62rpx;
  border: 1rpx solid #d8dfff;
  border-radius: 30rpx;
  background-color: #fff;
  font-size: 32rpx;
  text-align: center;
}

/* 下达 */
.ware_go{
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 30rpx 0;
}
.ware_go image{
  width: 734rpx;
  height: 144rpx;
}