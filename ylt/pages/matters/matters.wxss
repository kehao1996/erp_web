/* pages/matters/matters.wxss */
page{
  background: #f6f7fa;
}
/* 白胎采购 */
.wfpurchase{
  background-color: #Fff;
  border-radius: 30rpx;
  padding: 20rpx;
  margin: 20rpx;
}
.wfp_tile{
  width: 631rpx;
  height: 14rpx;
  margin: 40rpx auto;
  display: flex;
  align-items: center;
  justify-content: center;
  background-image:url('data:image/png;base64,');
  background-size: 100% 100%;
}
.wfp_tile image{
  display: block;
  width: 39rpx;
  height: 42rpx;
  margin-left: 10rpx;
}
.wfp_tile view{
  font-size: 42rpx;
  font-weight: bold;
  color: #081863;
  letter-spacing:10rpx;
  margin-left: 20rpx;
  line-height: 42rpx;
}
.wfp_item{
  background-color: #ecefff;
  border-radius: 30rpx;
  padding: 20rpx;
}
.wfp_item_i{
  display: flex;
  align-items: center;
  margin-top: 10rpx;
}
.wfp_item_i image{
  width: 47rpx;
  height: 50rpx;
  margin-right: 10rpx;
}
.wfp_item_i input{ 
  display: block;
  width: 43%;
  height: 70rpx;
  border: 1rpx solid #d8dfff;
  border-radius: 70rpx;
  background-color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 70rpx;
  box-sizing: border-box;
}
.wfp_item_name{
  width: 32%;
  font-size: 32rpx;
  color: #081863;
}
.wfp_item_btn{
  font-size: 32rpx;
  color:#081863;
  font-weight: bold;
  letter-spacing:5rpx;
  padding: 20rpx 5rpx 20rpx 24rpx;
}
/* 人物 */
.ren_back{
  background-image:url('data:image/png;base64,');

}
.ren_back view{
  margin: 0;
}
.ren_lei{
  font-size: 42rpx;
  font-weight: bold;
  color: #081863;
  letter-spacing: 10rpx;
  text-align: center;
  margin-bottom: 30rpx;
}
/* 退回两个按钮 */
.wfp_t_btn{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-top: 35rpx;
  padding-bottom: 20rpx;
}
.wfp_t_btn image{
  width: 254rpx;
  height: 70rpx;
}
.wfp_t_btn .wfp_t_tok{
  border: 1rpx solid #091965;
  padding: 5rpx 10rpx;
  border-radius: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.wfp_t_btn .wfp_t_tok view{
  width: 332rpx;
  height: 60rpx;
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  line-height: 60rpx;
  letter-spacing:5rpx;
  border-radius: 60rpx;
  border: 1rpx solid #091965;
  color: #091965;
  box-sizing: border-box;
}