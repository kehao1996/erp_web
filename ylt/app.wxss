/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 头部样式 */
.head{
  position: fixed;
  z-index: 99;
  padding-top: 40rpx;
  width: 750rpx;
  height: 128rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  background:url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.head image {
  position: absolute;
  z-index: 2;
  width: 59rpx;
  height: 86rpx;
  margin-left: 20rpx;
}
.head .head_text {
  width: 750rpx;
  position: absolute;
  z-index: 1;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
}
/* 通用个人简介 */
.top{
  position: relative;
  width: 750rpx;
  height: 333rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  background:url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.top .head_nameimg{
  display: block;
  position: absolute;
  top: 11rpx;
  left: 50%;
  transform: translate(-50%);
  width: 148rpx;
  height: 148rpx;
  border-radius: 50%;
  border: 1rpx solid #6b74a1;
}
.top .head_img{
  width: 100%;
  height: 105rpx;
  display: flex;
  justify-content: center;
  position: absolute;
  bottom: 70rpx;
}
.top .head_img image{
  width: 404rpx;
  height: 105rpx;
}
.top_text {
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  transform: translate(-50%);
  font-size: 32rpx;
}
.head_img_n{
  width: 384rpx;
  height: 85rpx;
  color: #081863;
  font-size: 32rpx;
  text-align: center;
  line-height: 85rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
/* 下达 */
.ware_go{
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 30rpx 0;
}
.ware_go image{
  width: 734rpx;
  height: 144rpx;
}

