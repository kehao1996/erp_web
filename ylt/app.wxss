/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 头部样式 */
.head{
  position: fixed;
  z-index: 99;
  padding-top: 40rpx;
  width: 750rpx;
  height: 128rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  background:url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.head image {
  position: absolute;
  z-index: 2;
  width: 59rpx;
  height: 86rpx;
  margin-left: 20rpx;
}
.head .head_text {
  width: 750rpx;
  position: absolute;
  z-index: 1;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
}
